{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c --environment BUILD:localdev",
    "build-prod": "rollup -c --environment BUILD:production",
    "local": "rollup -c -w --watch.include 'src/**' --environment BUILD:localdev",
    "remote": "rollup -c -w --watch.include 'src/**' --environment BUILD:remotedev",
    "server": "sirv public --single --dev",
    "serve-local": "run-p local server",
    "serve-remote": "run-p remote server",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv": "^1.0.11",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/node": "^14.14.14",
    "express": "^4.17.1",
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.5.0",
    "webrtc-adapter": "^7.7.0"
  }
}